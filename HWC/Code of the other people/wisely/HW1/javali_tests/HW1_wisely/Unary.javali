/* Tests Unary ops */
class Main {
	void main() {
		int i0,  i1,  i2;
		
		i0 = 0;
		i1 = 1;
		i2 = i0 + i1;
		
		write(-i2); writeln(); //-1
		write(+i2); writeln(); //1
		write(!i1); writeln(); //0
		write(!5); writeln();  //0
		write(!!5); writeln();  //1
		write(!i0); writeln();  //1

		
		i0 = i2 + (-i1) + (+i2) + i1 - i0; //2
		
		write(i0); writeln();
		
		write(--i1); writeln(); //1
	}
}