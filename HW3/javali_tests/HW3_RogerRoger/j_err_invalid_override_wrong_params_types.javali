/* test that overriding methods with different parameter types are rejected */

class Main {

    void main() {
        writeln();
    }

}


class B {

    void method(int a, boolean b) {
        write(a);
    }

}
class A extends B {

    void method(int a, int b) {
        write(a);
    }

}
